{
  "name": "@kubev2v/common",
  "version": "0.0.1",
  "description": "Common components and utilities for dynamic plugins",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubev2v/forklift-console-plugin.git",
    "directory": "packages/common"
  },
  "files": [
    "./dist/*"
  ],
  "exports": {
    "./components/*": "./dist/components/*",
    "./hooks/*": "./dist/hooks/*",
    "./utils/*": "./dist/utils/*"
  },
  "typesVersions": {
    "*": {
      "components/*": [
        "dist/components/*"
      ],
      "hooks/*": [
        "dist/hooks/*"
      ],
      "utils/*": [
        "dist/utils/*"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf coverage",
    "clean:all": "npm run clean && rm -rf node_modules",
    "build": "npx webpack build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:updateSnapshot": "jest --updateSnapshot",
    "indexify:overwrite": "npx yaindexer --overwrite -i ./src"
  },
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@migtools/lib-ui": "^8.4.1",
    "@openshift-console/dynamic-plugin-sdk": "^0.0.18",
    "@patternfly/react-core": "^4.224.1",
    "@patternfly/react-icons": "^4.75.1",
    "@patternfly/react-styles": "^4.74.1",
    "@patternfly/react-table": "^4.93.1",
    "@patternfly/react-tokens": "^4.76.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/history": "^4.7.11",
    "@types/jest": "^28.1.3",
    "@types/netmask": "^1.0.30",
    "@types/q": "^1.5.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dayjs": "^1.11.7",
    "file-saver": "^2.0.5",
    "history": "^4.10.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "netmask": "^2.0.2",
    "q": "^1.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.17.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.10.0"
  }
}
